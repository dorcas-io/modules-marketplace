{
    "name": "dorcas/modules-marketplace",
    "description": "Dorcas Hub Marketplace Module",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Ifeoluwa Olawoye",
            "email": "bolaji.olawoye@hostvillenigeria.org"
        }
    ],
    "require": {

        "php": ">=7.4",
        "aws/aws-sdk-php": "^3.74",
        "guzzlehttp/guzzle": "^7.4.5",
        "laravel/framework": "^8.0|^10.10"

    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Dorcas\\ModulesMarketplace\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dorcas\\ModulesMarketplace\\ModulesMarketplaceServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
